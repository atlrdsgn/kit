{
  "name": "@atlrdsgn/kit",
  "version": "0.4.92",
  "description": "AtelierKitÂ® React",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./types": {
      "import": "./dist/types/index.d.ts",
      "require": "./dist/types/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build",
    "build:types": "yarn tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "clean": "rm -rf node_modules && rm -rf dist && rm -rf prod.package/* && rm -rf .turbo",
    "release": "npm publish --access public",
    "dev:release": "npm publish --access private",
    "npm:login": "npm login --registry=https://npm.pkg.github.com --scope=@atlrdsgn",
    "pretty": "prettier --write src/**/*.{ts,tsx,json,md}"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-menubar": "^1.0.3",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-portal": "^1.0.3",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@atlrdsgn/tsconfig": "workspace:*",
    "@types/node": "^20.5.6",
    "@vanilla-extract/css": "^1.13.0",
    "@vanilla-extract/css-utils": "^0.1.3",
    "@vanilla-extract/dynamic": "^2.0.3",
    "@vanilla-extract/recipes": "^0.5.0",
    "@vanilla-extract/sprinkles": "^1.6.1",
    "@vanilla-extract/vite-plugin": "^3.9.0",
    "@vitejs/plugin-react": "^4.0.4",
    "eslint": "^8.47.0",
    "eslint-config-atlrdsgn": "workspace:*",
    "prettier": "^3.0.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-banner": "^0.7.0",
    "vite-plugin-dts": "^3.3.0",
    "vite-plugin-zip-pack": "^1.0.6"
  },
  "keywords": [],
  "author": "chvndler",
  "license": "MIT"
}
