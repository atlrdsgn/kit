{
  "name": "@atlrdsgn/kit",
  "version": "0.3.5",
  "description": "AtelierKitÂ® React",
  "main": "dist/index.umd.cjs",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    },
    "./css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./types": {
      "import": "./dist/types/index.d.ts",
      "require": "./dist/types/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build",
    "build:types": "yarn tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "lint": "eslint *.ts*",
    "clean": "rm -rf node_modules && rm -rf dist && rm -rf pkg/*",
    "release": "npm publish --access public",
    "dev:release": "npm publish --access private",
    "pretty": "prettier --write src/**/*.{ts,tsx,js,jsx,json,css,md}"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.12.0",
    "@vanilla-extract/css-utils": "^0.1.3",
    "@vanilla-extract/recipes": "^0.4.0",
    "@vanilla-extract/sprinkles": "^1.6.1",
    "clsx": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vanilla-extract/dynamic": "^2.0.3",
    "@vanilla-extract/vite-plugin": "^3.8.2",
    "@vitejs/plugin-react": "^4.0.3",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.4",
    "vite-plugin-banner": "^0.7.0",
    "vite-plugin-dts": "^3.3.0",
    "vite-plugin-zip-pack": "^1.0.6"
  },
  "keywords": [],
  "author": "chvndler",
  "license": "MIT"
}
