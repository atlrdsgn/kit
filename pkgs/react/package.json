{
  "name": "@atlrdsgn/kit",
  "version": "0.4.0",
  "description": "AtelierKitÂ® React",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./types": {
      "import": "./dist/types/index.d.ts",
      "require": "./dist/types/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build",
    "rollup": "rollup --config --bundleConfigAsCjs",
    "build:types": "yarn tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "lint": "eslint *.ts*",
    "clean": "rm -rf node_modules && rm -rf dist && rm -rf prod.package/*",
    "release": "npm publish --access public",
    "dev:release": "npm publish --access private",
    "npm:login": "npm login --registry=https://npm.pkg.github.com --scope=@atlrdsgn",
    "pretty": "prettier --write src/**/*.{ts,tsx,js,jsx,json,css,md}"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-portal": "^1.0.3",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-switch": "^1.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@vanilla-extract/css": "^1.12.0",
    "@vanilla-extract/css-utils": "^0.1.3",
    "@vanilla-extract/dynamic": "^2.0.3",
    "@vanilla-extract/recipes": "^0.4.0",
    "@vanilla-extract/sprinkles": "^1.6.1",
    "@vanilla-extract/vite-plugin": "^3.8.2",
    "@vitejs/plugin-react": "^4.0.3",
    "clsx": "^2.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.4",
    "vite-plugin-banner": "^0.7.0",
    "vite-plugin-dts": "^3.3.0",
    "vite-plugin-zip-pack": "^1.0.6"
  },
  "keywords": [],
  "author": "chvndler",
  "license": "MIT"
}
