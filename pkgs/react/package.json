    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-lib": "^0.0.5",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-banner": "^0.7.0",
    "vite-plugin-dts": "^3.5.3",
    "vite-plugin-zip-pack": "^1.0.6",
    "vite-tsconfig-paths": "^4.2.0"
  },
