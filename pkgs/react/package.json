{
  "name": "@atlrdsgn/kit",
  "version": "0.5.3",
  "description": "an everâ€“ expanding library of React components, primitives, and tools",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": [
    "src/lib/css/*",
    "src/lib/theme/*",
    "*.css.ts"
  ],
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build",
    "build:types": "yarn tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "empty": "rm -rf dist",
    "clean": "rm -rf node_modules && rm -rf dist && rm -rf .turbo",
    "release": "npm publish --access public",
    "dev:release": "npm publish --access private",
    "npm:login": "npm login --registry=https://npm.pkg.github.com --scope=@atlrdsgn",
    "pretti": "prettier --write src/**/*.{ts,tsx,json,md}"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
